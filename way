-- Gui to Lua
-- Version: 3.2

-- Instances:

local ScreenGui = Instance.new("ScreenGui")
local Main = Instance.new("Frame")
local MainLab = Instance.new("TextLabel")
local MakeWayPointFrame = Instance.new("Frame")
local MakePoint = Instance.new("TextButton")
local WaypointNameBox = Instance.new("TextBox")
local TextButton = Instance.new("TextButton")
local WayPointFrame = Instance.new("Frame")
local UIListLayout = Instance.new("UIListLayout")
local ClonePoint = Instance.new("TextButton")
local ClosePoint = Instance.new("TextButton")
local X1 = Instance.new("NumberValue")
local Y1 = Instance.new("NumberValue")
local Z1 = Instance.new("NumberValue")
--Properties:

ScreenGui.Parent = game.Players.LocalPlayer:WaitForChild("PlayerGui")
ScreenGui.ZIndexBehavior = Enum.ZIndexBehavior.Sibling

Main.Name = "Main"
Main.Parent = ScreenGui
Main.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
Main.Position = UDim2.new(0.361408889, 0, 0.321561337, 0)
Main.Size = UDim2.new(0, 226, 0, 35)

MainLab.Name = "MainLab"
MainLab.Parent = Main
MainLab.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
MainLab.Size = UDim2.new(0, 211, 0, 15)
MainLab.Font = Enum.Font.SourceSans
MainLab.Text = "Waypoint Gui"
MainLab.TextColor3 = Color3.fromRGB(0, 0, 0)
MainLab.TextSize = 14.000
MainLab.TextXAlignment = Enum.TextXAlignment.Left

MakeWayPointFrame.Name = "MakeWayPointFrame"
MakeWayPointFrame.Parent = Main
MakeWayPointFrame.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
MakeWayPointFrame.Position = UDim2.new(0, 0, 0.428571433, 0)
MakeWayPointFrame.Size = UDim2.new(0, 226, 0, 20)

MakePoint.Name = "MakePoint"
MakePoint.Parent = MakeWayPointFrame
MakePoint.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
MakePoint.Position = UDim2.new(0.78318584, 0, 0, 0)
MakePoint.Size = UDim2.new(0, 49, 0, 20)
MakePoint.Font = Enum.Font.SourceSans
MakePoint.Text = "Make Point"
MakePoint.TextColor3 = Color3.fromRGB(0, 0, 0)
MakePoint.TextScaled = true
MakePoint.TextSize = 14.000
MakePoint.TextWrapped = true

WaypointNameBox.Name = "WaypointNameBox"
WaypointNameBox.Parent = MakeWayPointFrame
WaypointNameBox.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
WaypointNameBox.Size = UDim2.new(0, 177, 0, 20)
WaypointNameBox.Font = Enum.Font.SourceSans
WaypointNameBox.Text = "Enter Name Here"
WaypointNameBox.TextColor3 = Color3.fromRGB(0, 0, 0)
WaypointNameBox.TextSize = 14.000

TextButton.Parent = MakeWayPointFrame
TextButton.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
TextButton.Position = UDim2.new(0.933628321, 0, -0.75, 0)
TextButton.Size = UDim2.new(0, 15, 0, 14)
TextButton.Font = Enum.Font.SourceSans
TextButton.Text = "X"
TextButton.TextColor3 = Color3.fromRGB(0, 0, 0)
TextButton.TextSize = 14.000

WayPointFrame.Name = "WayPointFrame"
WayPointFrame.Parent = Main
WayPointFrame.Active = true
WayPointFrame.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
WayPointFrame.ClipsDescendants = false
WayPointFrame.Position = UDim2.new(0, 0, 1, 0)
WayPointFrame.Size = UDim2.new(0, 226, 0, 0)

UIListLayout.Parent = WayPointFrame
UIListLayout.SortOrder = Enum.SortOrder.LayoutOrder

ClonePoint.Name = "ClonePoint"
ClonePoint.Parent = WayPointFrame
ClonePoint.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
ClonePoint.Position = UDim2.new(0, 0, NAN, 0)
ClonePoint.Size = UDim2.new(0, 206, 0, 20)
ClonePoint.Font = Enum.Font.SourceSans
ClonePoint.Text = "WayPointName"
ClonePoint.TextColor3 = Color3.fromRGB(0, 0, 0)
ClonePoint.TextSize = 14.000

ClosePoint.Name = "ClosePoint"
ClosePoint.Parent = ClonePoint
ClosePoint.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
ClosePoint.Position = UDim2.new(1, 0, 0, 0)
ClosePoint.Size = UDim2.new(0, 20, 0, 20)
ClosePoint.Font = Enum.Font.SourceSans
ClosePoint.Text = "X"
ClosePoint.TextColor3 = Color3.fromRGB(0, 0, 0)
ClosePoint.TextScaled = true
ClosePoint.TextSize = 14.000
ClosePoint.TextWrapped = true

X1.Parent = ClonePoint
X1.Name = "X1"
Y1.Parent = ClonePoint
Y1.Name = "Y1"
Z1.Parent = ClonePoint
Z1.Name = "Z1"
Main.Active = true
Main.Draggable = true
ClonePoint.Visible = false
local player = game.Players.LocalPlayer
MakePoint.MouseButton1Click:connect(function()
    local clone = ClonePoint:Clone()
    clone.Parent = WayPointFrame
    clone.Name = WaypointNameBox.Text
    clone.Text = WaypointNameBox.Text
    WaypointNameBox.Text = "Enter Name Here"
    clone.Visible = true
    clone.X1.Value = player.Character.HumanoidRootPart.Position.X
    clone.Y1.Value = player.Character.HumanoidRootPart.Position.Y
    clone.Z1.Value = player.Character.HumanoidRootPart.Position.Z
    local function butclicked()
        clone.MouseButton1Click:connect(function()
            player.Character.HumanoidRootPart.CFrame = CFrame.new(Vector3.new(clone.X1.Value,clone.Y1.Value,clone.Z1.Value))
        end)
        clone.ClosePoint.MouseButton1Click:connect(function()
            clone:Destroy()
        end)
    end
    coroutine.wrap(butclicked)()
end)


TextButton.MouseButton1Click:connect(function()
    if WayPointFrame.Visible == true then
        WayPointFrame.Visible = false
    else
        WayPointFrame.Visible = true
    end
end)
